<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>随手拍</name>
    <description>
        酷定吧附属项目
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        zhaofeihang
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="ldpi" src="res/icon/android/icon.png" />
        <icon density="mdpi" src="res/icon/android/icon.png" />
        <icon density="hdpi" src="res/icon/android/icon.png" />
        <icon density="xhdpi" src="res/icon/android/icon.png" />
        <icon density="xxhdpi" src="res/icon/android/icon.png" />
        <icon density="xxxhdpi" src="res/icon/android/icon.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon height="180" src="res/icon/ios/icon.png" width="180" />
        <icon height="60" src="res/icon/ios/icon.png" width="60" />
        <icon height="120" src="res/icon/ios/icon.png" width="120" />
        <icon height="76" src="res/icon/ios/icon.png" width="76" />
        <icon height="152" src="res/icon/ios/icon.png" width="152" />
        <icon height="40" src="res/icon/ios/icon.png" width="40" />
        <icon height="80" src="res/icon/ios/icon.png" width="80" />
        <icon height="57" src="res/icon/ios/icon.png" width="57" />
        <icon height="114" src="res/icon/ios/icon.png" width="114" />
        <icon height="72" src="res/icon/ios/icon.png" width="72" />
        <icon height="144" src="res/icon/ios/icon.png" width="144" />
        <icon height="29" src="res/icon/ios/icon.png" width="29" />
        <icon height="58" src="res/icon/ios/icon.png" width="58" />
        <icon height="50" src="res/icon/ios/icon.png" width="50" />
        <icon height="100" src="res/icon/ios/icon.png" width="100" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="xwalkMultipleApk" value="false" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-image-picker" spec="^1.1.3">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="选择上传图片" />
    </plugin>
    <plugin name="cordova-plugin-wechat" spec="^2.7.2">
        <variable name="WECHATAPPID" value="wx4ec18e420fe44790" />
    </plugin>
    <plugin name="cordova-plugin-payments-alipay" spec="^1.0.3">
        <variable name="APP_ID" value="2019031663535941" />
    </plugin>
    <plugin name="cordova-save-image-gallery" spec="https://github.com/agomezmoron/cordova-save-image-gallery.git" />
    <plugin name="cordova-android-support-gradle-release" spec="^2.1.0">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.0.1" />
    </plugin>
    <plugin name="cordova-plugin-imagepicker" spec="https://github.com/giantss/cordova-plugin-ImagePicker.git">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-crosswalk-webview" spec="^2.4.0">
        <variable name="XWALK_VERSION" value="23+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="^4.5.5" />
</widget>
